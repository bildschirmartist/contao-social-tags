{
  "name": "hofff/contao-social-tags",
  "description": "Define OpenGraph/twitter cards meta data in Contao",
  "license": "LGPL-3.0-or-later",
  "type": "contao-bundle",
  "keywords": [
    "facebook",
    "share",
    "like",
    "social",
    "opengraph",
    "twitter cards",
    "tags",
    "micro data",
    "meta data",
    "contao"
  ],
  "authors": [
    {
      "name": "Nicky Hoff",
      "email": "nick@hofff.com",
      "homepage": "https://www.hofff.com",
      "role": "Manager"
    },
    {
      "name": "David Molineus",
      "email": "david@hofff.com",
      "homepage": "https://www.hofff.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/hofff/contao-social-tags/issues",
    "irc": "irc://irc.freenode.org/contao.dev",
    "source": "https://github.com/hofff/contao-social-tags"
  },
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-pdo": "*",
    "contao/core-bundle": "^4.4",
    "doctrine/dbal": "^2.11 || ^3.0",
    "symfony/config": "^3.4 || ^4.0 || ^5.0",
    "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0",
    "symfony/http-foundation": "^3.4 || ^4.0 || ^5.0",
    "symfony/http-kernel": "^3.4 || ^4.0 || ^5.0",
    "symfony/routing": "^3.4 || ^4.0 || ^5.0"
  },
  "require-dev": {
    "contao/calendar-bundle": "^4.4",
    "contao/faq-bundle": "^4.4",
    "contao/news-bundle": "^4.4",
    "doctrine/coding-standard": "^8.0",
    "phpcq/runner-bootstrap": "^1.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Hofff\\Contao\\SocialTags\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.2.x-dev",
      "dev-master": "1.1.x-dev"
    },
    "contao-manager-plugin": "Hofff\\Contao\\SocialTags\\ContaoManager\\Plugin"
  }
}
